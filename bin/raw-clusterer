#!/bin/bash
# RAW Clusterer CLI wrapper script

# このスクリプトのディレクトリを取得
SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
# プロジェクトルートディレクトリ
PROJECT_DIR="$(dirname "$SCRIPT_DIR")"

# プロジェクトディレクトリに移動してuvを使用してPythonプログラムを実行
cd "$PROJECT_DIR" || exit 1
exec uv run python -m src.ui.cli.main "$@"
